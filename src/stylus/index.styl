body
  margin: 0
  background-color #E9573F

.container
  background-color #E6E9ED

.exam-paper
  font-size 16px
  .label
    margin-left 5px
    margin-right @margin-left
  ul.list-group
    li
      strong
        padding-top 8px
        padding-bottom @padding-top
      label
        @extend .exam-paper .label
        font-weight normal
        cursor pointer
      input
        margin 5px
